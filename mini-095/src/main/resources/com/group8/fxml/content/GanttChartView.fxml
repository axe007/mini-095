<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="ganttChartPane" prefWidth="${ganttChartPane.parent.width}" prefHeight="${ganttChartPane.parent.height}" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.group8.controllers.viewcontroller.GanttChartViewController">
   <children>
      <AnchorPane>
         <children>
            <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="findActivityLabel" focusTraversable="false" text="Find Activity">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin></Label>
                        <TextField fx:id="activitySearchTextField">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin></TextField>
                        <ComboBox fx:id="searchDomainComboBox" prefWidth="150.0">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin></ComboBox>
                        <Button fx:id="searchButton" focusTraversable="false" mnemonicParsing="false" text="Search" />
                     </children>
                  </HBox>
                  <TreeTableView fx:id="ganttChartTreeTableView" focusTraversable="false" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</StackPane>
